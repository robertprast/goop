# Environment Configuration
GOOP_ENV=development

# Server Configuration
PORT=8080
HOST=0.0.0.0
READ_TIMEOUT=30
WRITE_TIMEOUT=30
SHUTDOWN_TIMEOUT=5

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Authentication Configuration
GOOP_DISABLE_AUTH=false

# Database Configuration
DATABASE_URL=postgres://goop_user:goop_password@localhost:5432/goop?sslmode=disable

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE_URL=https://api.openai.com/v1

# Google Cloud / Gemini Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_OPENAI_BASE=https://generativelanguage.googleapis.com
GEMINI_OPENAI_VERSION=v1beta

# AWS Bedrock Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
